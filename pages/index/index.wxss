.map{
  position: fixed;
  left:0;
  right:0;
  top:0;
  bottom:80rpx;
}
.map map{
  width:100%;
  height:100%;
}
.footer{
  position: absolute;
  left: 0;
  right:0;
  bottom:10;
  display: flex
}
.footer view{
  flex:1;
  height:80rpx;
  line-height: 80rpx;
  text-align: center;
  background:#03bbd5;
  color:white
}
.footer view:nth-child(1){
  background: #ff9700
}
