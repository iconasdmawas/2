<!--index.wxml-->
<view class="map">
<map
  id="map"
  longitude="{{longitude}}"
  latitude="{{latitude}}"
  scale="14"
  controls="{{controls}}"
  bindcontroltap="controltap"
  markers="{{markers}}"
  bindmarkertap="markertap"
  polyline="{{polyline}}"
  bindregionchange="regionchange"
  show-location
></map>
<view class="footer">
<view bindtap="ms">发布</view>
<view bindtap='search'>搜索</view>
</view>
</view>
